{
  "name": "playwrightproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:unauth:firefox": "powershell -Command \"$env:BROWSER='firefox';$env:PW_PROJECT='unauthenticated'; npx playwright test --project=unauthenticated --reporter=line,allure-playwright\"",
    "test:unauth:chrome": "powershell -Command \"$env:BROWSER='chromium';$env:PW_PROJECT='unauthenticated'; npx playwright test --project=unauthenticated --reporter=line,allure-playwright\"",
    "test:auth:chrome": "powershell -Command \"$env:BROWSER='chromium';$env:PW_PROJECT='authenticated'; npx playwright test --project=authenticated --reporter=line,allure-playwright\"",
    "test:auth:firefox": "powershell -Command \"$env:BROWSER='firefox';$env:PW_PROJECT='authenticated'; npx playwright test --project=authenticated --reporter=line,allure-playwright\"",
    "test:both:chrome": "npm run test:unauth:chrome && npm run test:auth:chrome --reporter=line,allure-playwright",
    "test:both:firefox": "npm run test:unauth:firefox && npm run test:auth:firefox --reporter=line,allure-playwright"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "allure-playwright": "^3.4.2"
  }
}